{
  "name": "progress-tracker-v2",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "migrate": "bun --bun run ./src/lib/server/db/migrate.js",
    "generate": "drizzle-kit generate:sqlite --config=./drizzle.config.js",
    "studio": "bunx --bun drizzle-kit studio --config=./drizzle.config.js",
    "start": "bun run ./build",
    "dev": "bunx --bun vite dev",
    "build": "bunx --bun svelte-kit sync && bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "build:preview": "bunx --bun vite build && PORT=5173 bun ./build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "format": "biome format --write .",
    "fmt": "biome format --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@eslym/svelte-adapter-bun": "^0.5.9",
    "@sveltejs/kit": "^2.7.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.8",
    "@tailwindcss/vite": "^4.0.0-alpha.27",
    "@types/bun": "latest",
    "drizzle-kit": "^0.26.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.45.1",
    "eslint": "^9.12.0",
    "lucide-svelte": "^0.453.0",
    "nanoid": "^5.0.7",
    "prettier-plugin-svelte": "^3.2.7",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "prettier": "^4.0.0-alpha.10",
    "svelte-check": "^4.0.5",
    "svelte-headless-table": "^0.18.2",
    "svelte": "5.0.0-next.268",
    "tailwindcss": "^4.0.0-alpha.27",
    "typescript": "^5.6.3",
    "vite": "^6.0.0-beta.3"
  },
  "dependencies": {
    "bits-ui": "^0.21.16",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.35.1",
    "mode-watcher": "^0.4.1",
    "svelte-radix": "^2.0.0-next.5",
    "svelte-sonner": "^0.3.28",
    "tailwind-merge": "^2.5.4",
    "tailwind-variants": "^0.2.1",
    "vaul-svelte": "^0.3.2"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@sveltejs/kit",
    "esbuild",
    "es5-ext",
    "svelte-preprocess"
  ]
}
